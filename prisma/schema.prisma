// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model weather_geolocation {
  id               Int    @id @default(autoincrement())
  fullLocation     String @db.VarChar(255)
  location1        String @db.VarChar(255)
  location2        String @db.VarChar(255)
  locationCode     String @db.VarChar(255)
  positionNx       Float
  positionNy       Float
  locationEncoding String @db.VarChar(255)
}
